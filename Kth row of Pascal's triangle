public class Solution {
    public int[] getRow(int A) {
        int[] arr=new int[A+1];
        int count=1;
        for(int i=0;i<=A;i++){
            if(i==0||i==A){
                count=1;
                arr[i]=count;
            }else{
                count=count*(A-i+1)/i;
                arr[i]=count;
                            }
            
            
        }
        return arr;
    }
}
