public class Solution {
    public int isPower(int A) {
        if(A==1){
            return 1;
        }
        for(int i=2;i<=A/2;i++){
            float r =(float)(Math.log(A)/Math.log(i));
            if(r==Math.floor(r)){
                return 1;
            }
            
        }
        return 0;
    }
}



