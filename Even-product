public class Solution {
    public int solve(ArrayList<Integer> A) {
        
       
        if(A.size()==1){
            return 1;
        }
       int c=1;
        
            
            for(int i=0;i<A.size();i++){
                c=c*2;
                c=c%1000000007;
            }
        
        
        return c-1;
    }
}
